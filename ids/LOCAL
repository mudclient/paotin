#read framework/main.tin;

#var user[id]       LOCAL;
#var user[name]     本地测试;

#alias {auto-login} {
    setup;
    greeting;
};

#alias {setup} {
    #line quiet #port init null 0;
    #untick {KeepAlive};
    #alias {^%*$} {log.sendLog %%0} {9.999};
};

#alias {greeting} {
    okLog 欢迎使用 PaoTin++！;
    okLog 输入 HELP paotin 和 HELP xtintin 可以开始了解 PaoTin++。;
};

#alias {test} {
    load-module foo;
};

#alias {%*} {#showme 你奋笔疾书，挥毫落纸：「<060>%0<070>」} {9.999};
