load-lib telnet;
load-lib option;

option.Define {GMCPDebug} {Bool} {是否显示 GMCP 原始数据} {false};

#alias {gmcp.Enable} {
    #event {IAC WILL GMCP} {
        #send {$TELNET[IAC]$TELNET[DO]$TELNET[GMCP]\};
    };
};

#var gGMCP {};

#alias {gmcp.ToggleDebug} {option.Toggle GMCPDebug};
