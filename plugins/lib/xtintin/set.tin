///=== {
// #@ set.Intersection <é›†åˆ1> <é›†åˆ2> [...]
//    æ±‚ä¸¤ä¸ªæˆ–è€…å¤šä¸ªé›†åˆçš„äº¤é›†ã€‚
//    ğŸ“–  äº¤é›†æ˜¯æŒ‡ç”±åŒæ—¶å‡ºç°åœ¨æ‰€æœ‰é›†åˆä¸­çš„å…ƒç´ ç»„æˆçš„æ–°é›†åˆã€‚
//
//    EXAMPLE: \@set.Intersection{{\@set.Create{a;b;c}};{\@set.Create{b;c;d}};{\@set.Create{c;d;e}}}
//    RESULT: {{1}{c}}
// };
#func {set.Intersection} {
    #info arguments save;
    #unvar info[ARGUMENTS][0];
    #local argv {$info[ARGUMENTS]};

    #if { &argv[] < 2 } {
        #return {};
    };

    #local set  {$argv[1]};
    #local idx  {0};
    #loop {2} {&argv[]} {idx} {
        #if { &set[] == 0 } {
            #return {};
